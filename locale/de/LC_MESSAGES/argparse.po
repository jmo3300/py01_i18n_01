#: c:/dev/python37/Lib/argparse.py:297
msgid "usage: "
msgstr "Starten mit:"

#: c:/dev/python37/Lib/argparse.py:839
msgid ".__call__() not defined"
msgstr ".__call__() nicht definiert"

#: c:/dev/python37/Lib/argparse.py:1142
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "Unbekannter Parser %(parser_name)r (Auswahl: %(choices)s)"

#: c:/dev/python37/Lib/argparse.py:1196
msgid "argument \"-\" with mode %r"
msgstr "Argument \"-\" mit Modus %r"

#: c:/dev/python37/Lib/argparse.py:1204
msgid "can't open '%s': %s"
msgstr "'%s' kann nicht geöffnet werden: %s"

#: c:/dev/python37/Lib/argparse.py:1408
msgid "cannot merge actions - two groups are named %r"
msgstr ""
"Aktionen können nicht zusammengeführt werden - zwei Gruppen heißen %r"

#: c:/dev/python37/Lib/argparse.py:1446
msgid "'required' is an invalid argument for positionals"
msgstr "\"erforderlich\" ist ein ungültiges Argument für Positionen"

#: c:/dev/python37/Lib/argparse.py:1468
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"Ungültige Option %(option)r: muss mit einem Zeichen %(prefix_chars)r "
"beginnen"

#: c:/dev/python37/Lib/argparse.py:1488
msgid "dest= is required for options like %r"
msgstr "dest= ist für Optionen wie %r erforderlich"

#: c:/dev/python37/Lib/argparse.py:1505
msgid "invalid conflict_resolution value: %r"
msgstr "Ungültiger conflict_resolution-Wert: %r"

#: c:/dev/python37/Lib/argparse.py:1589
msgid "mutually exclusive arguments must be optional"
msgstr "sich gegenseitig ausschließende Argumente müssen optional sein"

#: c:/dev/python37/Lib/argparse.py:1652
msgid "positional arguments"
msgstr "Positionsargumente"

#: c:/dev/python37/Lib/argparse.py:1653
msgid "optional arguments"
msgstr "optionale Argumente"

#: c:/dev/python37/Lib/argparse.py:1668
msgid "show this help message and exit"
msgstr "zeige diese Hilfe-Nachricht an und beende das Programm"

#: c:/dev/python37/Lib/argparse.py:1699
msgid "cannot have multiple subparser arguments"
msgstr "mehrere subparser Argumente sind nicht zulässig"

#: c:/dev/python37/Lib/argparse.py:1751 c:/dev/python37/Lib/argparse.py:2257
msgid "unrecognized arguments: %s"
msgstr "unberücksichtigte Argumente: %s"

#: c:/dev/python37/Lib/argparse.py:1848
msgid "not allowed with argument %s"
msgstr "Mit Argument %s nicht zulässig"

#: c:/dev/python37/Lib/argparse.py:1894 c:/dev/python37/Lib/argparse.py:1908
msgid "ignored explicit argument %r"
msgstr "explizites Argument %r nicht berücksichtigt"

#: c:/dev/python37/Lib/argparse.py:2015
msgid "the following arguments are required: %s"
msgstr "Folgende Argumente sind notwendig: %s"

#: c:/dev/python37/Lib/argparse.py:2030
msgid "one of the arguments %s is required"
msgstr "Eines der der Argumente %s sind notwendig"

#: c:/dev/python37/Lib/argparse.py:2073
msgid "expected one argument"
msgstr "ein Argument ist erwartet"

#: c:/dev/python37/Lib/argparse.py:2074
msgid "expected at most one argument"
msgstr "höchstens ein Argument ist erwartet"

#: c:/dev/python37/Lib/argparse.py:2075
msgid "expected at least one argument"
msgstr "mindestens ein Argument ist erwartet"

#: c:/dev/python37/Lib/argparse.py:2137
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr ""
"mehrdeutige Option: %(option)s könnte übereinstimmen mit %(matches)s"

#: c:/dev/python37/Lib/argparse.py:2200
msgid "unexpected option string: %s"
msgstr "unerwartetes Optionsliteral: %s"

#: c:/dev/python37/Lib/argparse.py:2397
msgid "%r is not callable"
msgstr "%r ist nicht ausführbar"

#: c:/dev/python37/Lib/argparse.py:2414
msgid "invalid %(type)s value: %(value)r"
msgstr "unzulässiger %(type)s Wert: %(value)r"

#: c:/dev/python37/Lib/argparse.py:2425
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "ungültige Auswahl: %(value)r (gültig sind %(choices)s)"

#: c:/dev/python37/Lib/argparse.py:2501
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: Fehler: %(message)s\n"
